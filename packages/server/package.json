{
    "name": "ultra-discord-uniq-roles-bot",
    "version": "0.0.1",
    "description": "Ultra Uniq Bot for Discord Servers",
    "scripts": {
        "build": "tsc --build --clean && tsc ",
        "start": "tsc --build --clean && tsc && node dist/index.js",
        "dev": "concurrently \"npx ts-node ./src/index.ts --mode=dev\" \"npm --prefix ../client run dev\"",
        "test:file": "cross-env UNIT_TESTS=1 mocha --timeout 2000000 -r ts-node/register --exit",
        "test": "npm run test:file \"tests/**/*.spec.ts\""
    },
    "license": "ISC",
    "dependencies": {
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "cross-fetch": "^3.1.5",
        "discord.js": "^14.8.0",
        "dotenv": "^16.0.3",
        "enf-eosjs": "^23.0.0-rc4",
        "express": "^4.18.2",
        "mersenne-twister": "^1.1.0",
        "mongodb": "^5.8.0",
        "text-encoding": "^0.7.0"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/cors": "^2.8.13",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/mersenne-twister": "^1.1.2",
        "@types/node": "^18.15.3",
        "concurrently": "^7.6.0",
        "ts-node": "^10.8.0",
        "typescript": "^4.7.2"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": true,
        "trailingComma": "es5"
    }
}
